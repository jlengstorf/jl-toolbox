{
  "name": "jl-toolbox",
  "description": "CLI for common scripts",
  "author": "Jason Lengstorf <jason@lengstorf.com> (https://lengstorf.com)",
  "homepage": "https://github.com/kentcdodds/jl-toolbox#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/kentcdodds/jl-toolbox.git"
  },
  "bugs": {
    "url": "https://github.com/kentcdodds/jl-toolbox/issues"
  },
  "scripts": {
    "add-contributor": "node src contributors add",
    "test": "node src test",
    "test:update": "node src test --updateSnapshot",
    "build": "node src build",
    "lint": "node src lint",
    "format": "node src format",
    "validate": "node src validate",
    "precommit": "node src precommit"
  },
  "bin": {
    "jl-toolbox": "dist/index.js"
  },
  "files": ["dist", "babel.js", "eslint.js", "config.js"],
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  },
  "dependencies": {
    "all-contributors-cli": "^4.5.0",
    "arrify": "^1.0.1",
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-macros": "^1.0.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-minify-dead-code-elimination": "^0.2.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-inline-environment-variables": "^0.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.5",
    "cross-spawn": "^5.1.0",
    "doctoc": "^1.3.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-kentcdodds": "^13.0.0",
    "eslint-config-prettier": "^2.8.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "glob": "^7.1.2",
    "husky": "^0.14.3",
    "is-ci": "^1.0.10",
    "jest": "^21.2.1",
    "lint-staged": "^4.2.3",
    "lodash.camelcase": "^4.3.0",
    "lodash.has": "^4.5.2",
    "mkdirp": "^0.5.1",
    "prettier": "^1.8.2",
    "read-pkg-up": "^2.0.0",
    "resolve": "^1.4.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.48.2",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "which": "^1.3.0",
    "yargs-parser": "^8.0.0"
  },
  "devDependencies": {
    "jest-in-case": "^1.0.2",
    "slash": "^1.0.0"
  },
  "eslintConfig": {
    "extends": ["kentcdodds/jest", "airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "rules": {
      "no-process-exit": "off",
      "import/no-dynamic-require": "off",
      "import/no-unassigned-import": "off",
      "no-console": "off",
      "no-nested-ternary": "off",
      "prettier/prettier": [
        "error",
        { "bracketSpacing": true, "trailingComma": "all", "singleQuote": true }
      ]
    }
  },
  "eslintIgnore": ["node_modules", "coverage", "dist"],
  "keywords": [],
  "license": "MIT",
  "version": "0.0.0-semantically-released"
}
